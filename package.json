    "react-helmet-async": "^2.0.5",
    "react-router": "^6.22.0",
    "react-router-dom": "^6.22.0"
  },
  "devDependencies": {
    "@types/node": "^22.14.0",
    "@vercel/node": "^5.5.32",
    "@vitejs/plugin-react": "^5.0.0",
    "typescript": "~5.8.2",
    "vite": "^6.2.0"
  }
}
